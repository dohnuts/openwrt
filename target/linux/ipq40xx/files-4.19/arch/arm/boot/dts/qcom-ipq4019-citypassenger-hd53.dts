&blsp1_spi1 {
	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	status = "okay";
	reg-names = "spi_physical", "spi_bam_physical";
	reg = <0x78b5000 0x600>, <0x7884000 0x23000>;
	spi-max-frequency = <24000000>;
/* from base example , instead of gpio 12 in spi_0_pins */
	cs-gpios = <&tlmm 12 GPIO_ACTIVE_HIGH>;
/* Activating clock brok more ~
[    1.122619] spi_qup: probe of 78b5000.spi failed with error -2
*/
	// clocks = <&gcc GCC_BLSP1_QUP1_SPI_APPS_CLK>, <&gcc GCC_BLSP1_AHB_CLK>;
	// clock-names = "core_clk", "iface_clk";
	flash: m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		compatible = "w25q256,spi-nor";
		linux,modalias = "m25p80", "w25q256", "n25q128a11";
		spi-max-frequency = <24000000>;
		partitions {
			compatible = "fixed-partitions";
			partition@0 {
				label = "SBL1";
				reg = <0x0 0x40000>;
				read-only;
			};

			partition@40000 {
				label = "MIBIB";
				reg = <0x40000 0x20000>;
				read-only;
			};

			partition@60000 {
				label = "QSEE";
				reg = <0x60000 0x60000>;
				read-only;
			};

			partition@c0000 {
				label = "CDT";
				reg = <0xc0000 0x10000>;
				read-only;
			};

			partition@d0000 {
				label = "DDRPARAMS";
				reg = <0xd0000 0x10000>;
				read-only;
			};

			partition@e0000 {
				label = "APPSBLENV";
				reg = <0xe0000 0x10000>;
				read-only;
			};

			partition@f0000 {
				label = "APPSBL";
				reg = <0xf0000 0x80000>;
				read-only;
			};

			partition@170000 {
				label = "ART";
				reg = <0x170000 0x10000>;
				read-only;
			};

			partition@180000 {
				label = "HLOS";
				reg = <0x180000 0x400000>; // trying to let openwrt do his thingy\
				// reg = <0x180000 0x1A00000>;
				// compatible = "denx,fit";
				// read-only;
			};

			// trying to let openwrt do his thingy
			partition@580000 {
				label = "firmware";
				compatible = "squashfs,jffs2";
				// compatible = "denx,uimage";
				// read-only;
				reg = <0x580000 0x1600000>;
			};

			// DONT DO  THIS apparently
			//overlay@006d0000 {
			//	label = "overlay";
			//	reg = <0x006d0000 00010000>;
			//};
		};
	};
};

